{
  "name": "react-app",
  "version": "0.10.35",
  "description": "React app for use with iTwin Mobile SDK samples.",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/mobile-samples"
  },
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "cross-env TRANSPILE_DEPS=false DISABLE_TERSER=true USE_FAST_SASS=true BROWSER=none DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "start:camera": "cross-env REACT_APP_BUILD_TARGET=Camera TRANSPILE_DEPS=false DISABLE_TERSER=true USE_FAST_SASS=true BROWSER=none DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "copy:assets": "cpx \"./node_modules/@itwin/mobile-sdk-core/lib/public/locales/**\" ./build/locales && cpx \"./node_modules/@itwin/mobile-ui-react/lib/public/locales/**\" ./build/locales",
    "copy:camera": "cpx \"./src/frontend/CameraSample/public/locales/**\" ./build/locales",
    "build:frontend": "cross-env NODE_OPTIONS=--max_old_space_size=8192 TRANSPILE_DEPS=false DISABLE_TERSER=true USE_FAST_SASS=true DISABLE_ESLINT_PLUGIN=true react-scripts build && run-s copy:assets",
    "build:camera": "cross-env REACT_APP_BUILD_TARGET=Camera NODE_OPTIONS=--max_old_space_size=8192 TRANSPILE_DEPS=false DISABLE_TERSER=true USE_FAST_SASS=true DISABLE_ESLINT_PLUGIN=true react-scripts build && run-s copy:assets copy:camera",
    "compile:backend": "cross-env tsc -p tsconfig.backend.json",
    "webpack:backend": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config ./webpack.config.js --env outdir=./lib/webpack --env entry=./lib/backend/main.js --env bundlename=main",
    "build:backend": "run-s compile:backend webpack:backend",
    "build": "run-p build:frontend build:backend",
    "clean": "rimraf build lib",
    "lint": "eslint -f visualstudio \"./src/**/*.{ts,tsx}\" 1>&2",
    "synclocalbuilds": "cd ../../../mobile-sdk-core && npm run synctotestapp && cd ../mobile-ui-react && npm run synctotestapp"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@bentley/react-scripts": "5.0.3",
    "@itwin/build-tools": "3.6.0",
    "@itwin/eslint-plugin": "3.6.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^12.20.30",
    "@types/react": "^16.14.17",
    "@types/react-dom": "^16.8.4",
    "cpx2": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-testing-library": "^5.9.1",
    "fibers": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^0.1.1",
    "sass": "^1.39.0",
    "typescript": "^4.4.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@itwin/appui-abstract": "3.6.0",
    "@itwin/appui-layout-react": "3.6.0",
    "@itwin/appui-react": "3.6.0",
    "@itwin/components-react": "3.6.0",
    "@itwin/core-backend": "3.6.0",
    "@itwin/core-bentley": "3.6.0",
    "@itwin/core-common": "3.6.0",
    "@itwin/core-frontend": "3.6.0",
    "@itwin/core-geometry": "3.6.0",
    "@itwin/core-i18n": "3.6.0",
    "@itwin/core-markup": "3.6.0",
    "@itwin/core-mobile": "3.6.0",
    "@itwin/core-orbitgt": "3.6.0",
    "@itwin/core-quantity": "3.6.0",
    "@itwin/core-react": "3.6.0",
    "@itwin/ecschema-metadata": "3.6.0",
    "@itwin/imodel-components-react": "3.6.0",
    "@itwin/imodels-access-backend": "2.2.1",
    "@itwin/imodels-access-frontend": "2.2.1",
    "@itwin/imodels-client-management": "2.2.1",
    "@itwin/linear-referencing-backend": "3.6.0",
    "@itwin/linear-referencing-common": "3.6.0",
    "@itwin/measure-tools-react": "^0.8.0",
    "@itwin/mobile-sdk-core": "0.10.35",
    "@itwin/mobile-ui-react": "0.10.35",
    "@itwin/presentation-backend": "3.6.0",
    "@itwin/presentation-common": "3.6.0",
    "@itwin/presentation-components": "3.6.0",
    "@itwin/presentation-frontend": "3.6.0",
    "@itwin/projects-client": "0.7.0",
    "@itwin/webgl-compatibility": "3.6.0",
    "axios": "^0.25.0",
    "base64-js": "^1.5.1",
    "classnames": "^2.2.6",
    "js-base64": "^3.7.2",
    "jsonwebtoken": "^9.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.4",
    "redux": "^4.0.5",
    "typedoc": "^0.22.11"
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": [
      "plugin:@itwin/ui",
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "off",
      "@itwin/no-internal-barrel-imports": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-static-element-interactions": "off"
    },
    "parserOptions": {
      "project": [
        "./tsconfig.json",
        "./tsconfig.backend.json"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
