    "@itwin/mobile-sdk-core": "0.24.0",
    "@itwin/mobile-ui-react": "0.24.0",
    "@itwin/presentation-backend": "5.0.0",
    "@itwin/presentation-common": "5.0.0",
    "@itwin/presentation-components": "5.12.3",
    "@itwin/presentation-frontend": "5.0.0",
