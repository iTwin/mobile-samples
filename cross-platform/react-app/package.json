    "@itwin/core-mobile": "5.0.0",
    "@itwin/core-orbitgt": "5.0.0",
    "@itwin/core-quantity": "5.0.0",
    "@itwin/core-react": "5.10.0",
    "@itwin/ecschema-metadata": "5.0.0",
    "@itwin/imodel-components-react": "5.10.0",
    "@itwin/imodels-access-backend": "6.0.0",
    "@itwin/imodels-access-common": "6.0.0",
    "@itwin/imodels-access-frontend": "6.0.0",
    "@itwin/imodels-client-management": "6.0.0",
    "@itwin/itwins-client": "1.6.1",
