    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "relativeDependencies": {
    "@itwin/mobile-sdk-core": "../../../mobile-sdk-core",
    "@itwin/mobile-ui-react": "../../../mobile-ui-react"
  }
}
