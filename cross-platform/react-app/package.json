{
  "name": "react-app",
  "version": "0.20.1",
  "description": "React app for use with iTwin Mobile SDK samples.",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/mobile-samples"
  },
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "cross-env TRANSPILE_DEPS=false DISABLE_TERSER=true USE_FAST_SASS=true BROWSER=none USING_NPM=true react-scripts start",
    "start:camera": "cross-env REACT_APP_BUILD_TARGET=Camera run-s start",
    "copy:assets": "cpx \"./node_modules/@itwin/mobile-sdk-core/lib/public/locales/**\" ./build/locales && cpx \"./node_modules/@itwin/mobile-ui-react/lib/public/locales/**\" ./build/locales",
    "copy:camera": "cpx \"./src/frontend/CameraSample/public/locales/**\" ./build/locales",
    "build:frontend": "cross-env NODE_OPTIONS=--max_old_space_size=8192 TRANSPILE_DEPS=false DISABLE_TERSER=true USE_FAST_SASS=true USING_NPM=true react-scripts build && run-s copy:assets",
    "build:camera": "cross-env REACT_APP_BUILD_TARGET=Camera run-s build:frontend copy:camera",
    "compile:backend": "cross-env tsc -p tsconfig.backend.json",
    "webpack:backend": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config ./webpack.config.js --env outdir=./lib/webpack --env entry=./lib/backend/main.js --env bundlename=main",
    "build:backend": "run-s compile:backend webpack:backend",
    "build": "run-p build:frontend build:backend",
    "clean": "rimraf build lib",
    "lint": "eslint -f visualstudio \"./src/**/*.{ts,tsx}\" 1>&2"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@bentley/react-scripts": "5.0.4",
    "@itwin/build-tools": "4.1.0",
    "@itwin/eslint-plugin": "4.0.0-dev.33",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^12.20.30",
    "@types/react": "^16.14.17",
    "@types/react-dom": "^16.8.4",
    "cpx2": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "npm-run-all": "^4.1.5",
    "null-loader": "^0.1.1",
    "sass": "^1.39.0",
    "typedoc": "~0.22.0",
    "typescript": "~5.0.2",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@itwin/appui-abstract": "4.1.0",
    "@itwin/appui-layout-react": "4.4.0",
    "@itwin/appui-react": "4.4.0",
    "@itwin/components-react": "4.4.0",
    "@itwin/core-backend": "4.1.0",
    "@itwin/core-bentley": "4.1.0",
    "@itwin/core-common": "4.1.0",
    "@itwin/core-frontend": "4.1.0",
    "@itwin/core-geometry": "4.1.0",
    "@itwin/core-i18n": "4.1.0",
    "@itwin/core-markup": "4.1.0",
    "@itwin/core-mobile": "4.1.0",
    "@itwin/core-orbitgt": "4.1.0",
    "@itwin/core-quantity": "4.1.0",
    "@itwin/core-react": "4.4.0",
    "@itwin/core-telemetry": "4.1.0",
    "@itwin/ecschema-metadata": "4.1.0",
    "@itwin/imodel-components-react": "4.4.0",
    "@itwin/imodels-access-backend": "4.0.4",
    "@itwin/imodels-access-frontend": "4.0.4",
    "@itwin/imodels-client-management": "4.1.1",
    "@itwin/itwins-client": "1.2.0",
    "@itwin/linear-referencing-backend": "4.1.0",
    "@itwin/linear-referencing-common": "4.1.0",
    "@itwin/measure-tools-react": "^0.13.0",
    "@itwin/mobile-sdk-core": "0.20.1",
    "@itwin/mobile-ui-react": "0.20.1",
    "@itwin/presentation-backend": "4.1.0",
    "@itwin/presentation-common": "4.1.0",
    "@itwin/presentation-components": "4.1.0",
    "@itwin/presentation-frontend": "4.1.0",
    "@itwin/webgl-compatibility": "4.1.0",
    "@react-hook/resize-observer": "^1.2.6",
    "axios": "^0.25.0",
    "base64-js": "^1.5.1",
    "classnames": "^2.2.6",
    "js-base64": "^3.7.2",
    "jsonwebtoken": "^9.0.0",
    "react": "^17.0.0",
    "redux": "^4.0.5"
  },
  "overrides": {
    "@bentley/react-scripts": {
      "typescript": "~5.0.2"
    },
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-jsdoc": {
      "eslint": "^8.36.0"
    },
    "fast-sass-loader": {
      "loader-utils": "^2.0.0"
    },
    "svg-baker": {
      "loader-utils": "^2.0.0",
      "postcss": "7.0.39"
    },
    "svg-sprite-loader": {
      "loader-utils": "^2.0.0"
    },
    "typedoc": {
      "typescript": "~5.0.2"
    }
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": [
      "plugin:@itwin/ui"
    ],
    "rules": {
      "no-console": "off",
      "@itwin/no-internal-barrel-imports": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-static-element-interactions": "off"
    },
    "parserOptions": {
      "project": [
        "./tsconfig.json",
        "./tsconfig.backend.json"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "relativeDependencies": {
    "@itwin/mobile-sdk-core": "../../../mobile-sdk-core",
    "@itwin/mobile-ui-react": "../../../mobile-ui-react"
  }
}
